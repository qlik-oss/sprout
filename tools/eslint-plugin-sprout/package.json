{
  "name": "@qlik/eslint-plugin-sprout",
  "version": "1.1.0",
  "homepage": "https://github.com/qlik/sprout-kit#readme",
  "bugs": {
    "url": "https://github.com/qlik/sprout-kit/issues"
  },
  "repository": "https://github.com/qlik/sprout-kit.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs"
    }
  },
  "main": "./src/index.mjs",
  "scripts": {
    "clean": "echo clean",
    "format:check": "prettier --check ./** --ignore-unknown",
    "format:write": "prettier --write ./** --ignore-unknown",
    "lint": "eslint .",
    "lint:ci": "eslint . -f json -o eslint-report.json",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@qlik/eslint-config": "^1.4.18",
    "@qlik/tsconfig": "^0.3.1",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^9.35.0"
  }
}
