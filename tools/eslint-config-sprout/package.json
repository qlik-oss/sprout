{
  "name": "@qlik/eslint-config-sprout",
  "version": "1.0.0",
  "homepage": "https://github.com/qlik-trial/sprout-css-kit#readme",
  "bugs": {
    "url": "https://github.com/qlik-trial/sprout-css-kit/issues"
  },
  "repository": "https://github.com/qlik-trial/sprout-css-kit.git",
  "license": "Apache 2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs"
    },
    "./react": {
      "types": "./src/react.d.ts",
      "import": "./src/react.mjs"
    },
    "./rules": {
      "types": "./src/rules.d.ts",
      "import": "./src/rules.mjs"
    }
  },
  "main": "./src/index.mjs",
  "types": "./src/index.d.ts",
  "scripts": {
    "clean": "echo clean",
    "format:check": "prettier --check ./** --ignore-unknown",
    "format:write": "prettier --write ./** --ignore-unknown",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "dependencies": {
    "@qlik/eslint-config": "^1.4.15",
    "@qlik/eslint-plugin-sprout": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "eslint-plugin-react": "^7.37.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0"
  },
  "peerDependencies": {
    "eslint": "^9.35.0"
  }
}
