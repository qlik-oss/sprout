{
  "name": "@qlik/eslint-config-sprout",
  "version": "1.0.3",
  "homepage": "https://github.com/qlik/sprout#readme",
  "bugs": {
    "url": "https://github.com/qlik/sprout/issues"
  },
  "repository": "https://github.com/qlik/sprout.git",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.mjs"
    },
    "./react": {
      "types": "./src/react.d.ts",
      "import": "./src/react.mjs"
    },
    "./rules": {
      "types": "./src/rules.d.ts",
      "import": "./src/rules.mjs"
    }
  },
  "main": "./src/index.mjs",
  "types": "./src/index.d.ts",
  "scripts": {
    "format:check": "prettier --check ./** --ignore-unknown",
    "format:write": "prettier --write ./** --ignore-unknown",
    "lint": "eslint .",
    "lint:ci": "eslint . -f json -o eslint-report.json",
    "test": "vitest run"
  },
  "dependencies": {
    "@qlik/eslint-config": "^1.4.16",
    "@qlik/eslint-plugin-sprout": "workspace:*",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "eslint-plugin-react": "^7.37.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "eslint": "^9.35.0"
  }
}
