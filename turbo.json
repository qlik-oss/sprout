{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["lib/**", "dist/**", "generated/**", "react/**", "raw/**", "vue/**"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    },
    "start": {},
    "storybook:build": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {},
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**", "test-results/**", "playwright-report/**"]
    },
    "format:write": {},
    "format:check": {}
  }
}
