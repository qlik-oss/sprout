        "l": {
          "$type": "spacing",
          "$value": "{theming.common.density.l} * {sprout.common.scale.density}",
          "$description": "L density"
        },
        "xl": {
          "$type": "spacing",
          "$value": "{theming.common.density.xl} * {sprout.common.scale.density}",
