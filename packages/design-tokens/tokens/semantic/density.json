        "l": {
          "$type": "spacing",
          "$value": "{theming.common.spacing.l} * {sprout.common.scale.spacing}",
          "$description": "L spacing"
        },
        "xl": {
          "$type": "spacing",
          "$value": "{theming.common.spacing.xl} * {sprout.common.scale.spacing}",
