        "xxl": {
          "$type": "spacing",
          "$value": "{theming.common.density.xxl} * {sprout.common.scale.density}",
          "$description": "XXL density"
        },
        "3xl": {
          "$type": "spacing",
          "$value": "{theming.common.density.3xl} * {sprout.common.scale.density}",
          "$description": "3XL density"
        }
      },
      "scale": {
        "spacing": {
          "$type": "number",
          "$value": "1",
          "$description": "(Variable only). Scaling property for spacing tokens"
        },
        "density": {
          "$type": "number",
          "$value": "1",
          "$description": "(Variable only). Scaling property for density tokens"
        }
      }
    }
  }
