{
  "sprout": {
    "common": {
      "spacing": {
        "xs": {
          "$type": "spacing",
          "$value": "{theming.common.spacing.xs} * {sprout.common.scale.spacing}",
          "$description": "XS spacing"
        },
        "s": {
          "$type": "spacing",
          "$value": "{theming.common.spacing.s} * {sprout.common.scale.spacing}",
          "$description": "S spacing"
        },
        "m": {
          "$type": "spacing",
          "$value": "{theming.common.spacing.m} * {sprout.common.scale.spacing}",
          "$description": "M spacing. Default grid unit"
