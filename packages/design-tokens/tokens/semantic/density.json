        "3xl": {
          "$type": "spacing",
          "$value": "{theming.common.spacing.3xl} * {sprout.common.scale.spacing}",
          "$description": "3XL spacing"
        }
      },
      "density": {
        "xs": {
          "$type": "spacing",
          "$value": "{theming.common.density.xs} * {sprout.common.scale.density}",
          "$description": "Extra small density"
        },
        "s": {
          "$type": "spacing",
          "$value": "{theming.common.density.s} * {sprout.common.scale.density}",
