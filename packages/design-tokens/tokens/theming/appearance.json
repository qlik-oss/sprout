{
  "theming": {
    "common": {
      "foreground-color": {
        "default": {
          "$type": "color",
          "$value": "{qlik.appearance.fg}",
          "$description": "Primary text color"
        },
        "weak": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "space": "srgb",
                "value": "0.76"
              }
            }
          },
          "$type": "color",
          "$value": "{qlik.appearance.fg}",
          "$description": "Secondary text color. Used for discrete text."
        },
        "disabled": {
          "$extensions": {
            "studio.tokens": {
              "modify": {
                "type": "alpha",
                "space": "srgb",
                "value": "{qlik.appearance.mix.default} "
              }
            }
          },
          "$type": "color",
          "$value": "{theming.common.foreground-color.default}",
          "$description": "Disabled text color."
        }
      },
      "background-color": {
        "default": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.default}",
          "$description": "First level surface color."
        },
        "weak": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.weak}",
          "$description": "Second level surface color."
        },
        "moderate": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.moderate}",
          "$description": "Third level surface color."
        },
        "strong": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.strong}",
          "$description": "Fourth level surface color."
        },
        "enabled": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.enabled}",
          "$description": "Default background color for enabled interactive elements"
        },
        "hover": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.hover}",
          "$description": "Hover background color for interactive elements"
        },
        "pressed": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.pressed}",
          "$description": "Pressed background color for interactive elements"
        },
        "toggled": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.toggled}",
          "$description": "Background color for toggled state of interactive elements"
        },
        "disabled": {
          "$type": "color",
          "$value": "{qlik.appearance.background-color.disabled}",
          "$description": "Disabled background color for interactive elements"
        }
      },
      "border": {
        "default": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.border-color.default}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Baseline border style for general contours and structural outlines. Used for neutral objects when no special state (focus, hover, disabled) is active."
        },
        "weak": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.border-color.weak}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Applied to disabled states (buttons, text fields) or secondary surfaces where contrast should be reduced."
        },
        "moderate": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.border-color.moderate}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Primary, WCAG-compliant border style. Used for active and enabled interactive elements such as buttons, text fields, and inputs. Ensures accessible contrast against backgrounds."
        },
        "strong": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.border-color.strong}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Emphasized border style for interaction and highlight. Used for hover states, or cases where stronger separation is required (e.g., hovered text fields, active controls)."
        },
        "extra-strong": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.border-color.extra-strong}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "High-emphasis border style for critical visibility. Reserved for toggle indicators, or high-contrast / accessibility needs."
        }
      },
      "elevation": {
        "default": {
          "$type": "boxShadow",
          "$value": [
            {
              "x": "0",
              "y": "2",
              "blur": "4",
              "spread": "0",
              "color": "{palette.black-alpha.15}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            },
            {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "1",
              "color": "{palette.black-alpha.05}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            }
          ],
          "$description": "Default elevation box shadow style"
        },
        "weak": {
          "$type": "boxShadow",
          "$value": [
            {
              "x": "0",
              "y": "1",
              "blur": "2",
              "spread": "0",
              "color": "{palette.black-alpha.15}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            },
            {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "1",
              "color": "{palette.black-alpha.05}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            }
          ],
          "$description": "Weak elevation box shadow style"
        },
        "moderate": {
          "$type": "boxShadow",
          "$value": [
            {
              "x": "0",
              "y": "4",
              "blur": "10",
              "spread": "0",
              "color": "{palette.black-alpha.15}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            },
            {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "1",
              "color": "{palette.black-alpha.05}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            }
          ],
          "$description": "Moderate elevation box shadow style"
        },
        "strong": {
          "$type": "boxShadow",
          "$value": [
            {
              "x": "0",
              "y": "6",
              "blur": "20",
              "spread": "0",
              "color": "{palette.black-alpha.15}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            },
            {
              "x": "0",
              "y": "0",
              "blur": "0",
              "spread": "1",
              "color": "{palette.black-alpha.05}",
              "type": "dropShadow",
              "module": "qlik.appearance"
            }
          ],
          "$description": "Strong elevation box shadow style"
        }
      },
      "divider": {
        "default": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.default}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Baseline divider style for standard separation. Used to clearly separate related but distinct content blocks (e.g., between list items, form sections, or stacked cards). Provides a neutral level of contrast suitable for most layouts."
        },
        "subtle": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.default}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.dashed}"
          },
          "$description": "Compound token for subtle divider style"
        },
        "subtle-weak": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.weak}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.dashed}"
          },
          "$description": "Compound token for a very subtle divider style"
        },
        "weak": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.weak}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Reduced-contrast divider for low-emphasis boundaries. Suitable for secondary layout divisions, soft grid lines, or visual rhythm where a stronger divider would be distracting."
        },
        "moderate": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.moderate}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Compound token for moderate border style"
        },
        "strong": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.strong}",
            "width": "{qlik.appearance.border-width.default}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Compound token for strong border style"
        },
        "extra-strong": {
          "$type": "border",
          "$value": {
            "color": "{qlik.appearance.divider-color.extra-strong}",
            "width": "{qlik.appearance.border-width.strong}",
            "style": "{qlik.appearance.border-style.default}"
          },
          "$description": "Maximum emphasis divider for critical or high-contrast requirements."
        }
      }
    },
